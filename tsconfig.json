{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": [
    "./src/*.ts",
    "./src/**/*.ts",
    "env.d.ts"
  ],
  "exclude": ["node_modules"]
}
